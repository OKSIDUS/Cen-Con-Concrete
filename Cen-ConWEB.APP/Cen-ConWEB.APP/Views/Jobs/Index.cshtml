@using Cen_ConWEB.DAL.DataContext.Entity
@model List<Job>

<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>ClientId</th>
            <th>Location</th>
            <th>SquareFeet</th>
            <th>Depth</th>
            <th>OrderId</th>
            <th>ConcreteSupplierId</th>
            <th>PourType</th>
            <th>FinishTypeId</th>
            <th>JobType</th>
            <th>StatusId</th>
            <th>CreatedAt</th>
            <th>UpdatedAt</th>
            <th>CrewId</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var job in Model)
        {
            <tr>
                <td>@job.Id</td>
                <td>@job.ClientName</td>
                <td>@job.Location</td>
                <td>@job.SquareFeet</td>
                <td>@job.Depth</td>
                <td>@job.ConcreteCustomer</td>
                <td>@job.ConcreteSupplier</td>
                <td>@job.PourType</td>
                <td>@job.FinishType</td>
                <td>@job.JobTypeName</td>
                <td>@job.Status</td>
                <td>@job.CreatedAt</td>
                <td>@job.UpdatedAt</td>
                <td>@job.CrewName</td>
            </tr>
        }
    </tbody>
</table>
