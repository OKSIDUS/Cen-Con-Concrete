@using Cen_ConWEB.DAL.DataContext.Entity
@model Job

<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>ClientId</th>
            <th>Location</th>
            <th>SquareFeet</th>
            <th>Depth</th>
            <th>OrderId</th>
            <th>ConcreteSupplierId</th>
            <th>PourType</th>
            <th>FinishTypeId</th>
            <th>JobType</th>
            <th>StatusId</th>
            <th>CreatedAt</th>
            <th>UpdatedAt</th>
            <th>CrewId</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>@Model.Id</td>
            <td>@Model.ClientName</td>
            <td>@Model.Location</td>
            <td>@Model.SquareFeet</td>
            <td>@Model.Depth</td>
            <td>@Model.ConcreteCustomer</td>
            <td>@Model.ConcreteSupplier</td>
            <td>@Model.PourType</td>
            <td>@Model.FinishType</td>
            <td>@Model.JobTypeName</td>
            <td>@Model.Status</td>
            <td>@Model.CreatedAt</td>
            <td>@Model.UpdatedAt</td>
            <td>@Model.CrewName</td>
        </tr>
    </tbody>
</table>
