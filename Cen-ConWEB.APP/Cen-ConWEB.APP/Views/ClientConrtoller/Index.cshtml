@using Cen_ConWEB.BAL.Dtos.Types
@model List<ClientDto>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Phone Number</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var job in Model)
        {
            <tr>
                <td>@job.Id</td>
                <td>@job.FirstName</td>
                <td>@job.LastName</td>
                <td>@job.Email</td>
                <td>@job.PhoneNumber</td>
            </tr>
        }
    </tbody>
</table>
